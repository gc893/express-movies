
<%- include('../partials/header') %>
  <table id='list'>
    <thead>
      <tr>
        <th>Title</th>
        <th>Release Year</th>
      </tr>
    </thead>
    <tbody>
        <% movies.forEach(m => { %>
            <tr>
                <td><%= m.title %></td>
                <td><%= m.releaseYear %></td>
                <td><a href="/movies/<%= m._id %>">DETAILS</a></td>
              </tr>
        <% }); %>
    </tbody>
  </table>

  <%- include('../partials/footer') %>